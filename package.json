{
  "name": "askc-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "codegen": "npm run codegen:proto",
    "codegen:proto": "node scripts/generate-generated.cjs --config Actr.toml --proto-root askaway-proto --out src/generated --dist-import @actor-rtc/actr",
    "dev": "tsx src/main.ts",
    "start": "node dist/main.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@actor-rtc/actr": "https://github.com/actor-rtc/actr-ts/releases/download/v0.1.4/actor-rtc-actr-0.1.4.tgz"
  },
  "devDependencies": {
    "@iarna/toml": "^3.0.0",
    "@types/node": "^25.0.0",
    "protobufjs": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  }
}
